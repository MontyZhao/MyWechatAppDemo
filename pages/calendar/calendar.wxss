/* pages/calendar/calendar.wxss */
.title {
    text-align: center;
    line-height: 88rpx;
}
.action-wrap {
    display: flex;
    margin-bottom: 20rpx;
}
.line-wrap {
    display: flex;
    flex-wrap: wrap;
    width: 714rpx;
    /* border-right: 2rpx solid #AFAAA5; */
}

.item {
    width: 100rpx;
    height: 100rpx;
    border-left: 2rpx solid #AFAAA5;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-bottom: 2rpx solid #AFAAA5;
    font-size: 30rpx;
    font-weight: 400;
    color: #272A14;
}
.item .small {
    font-size: 24rpx;
}
.item.style1 {
    background: #FFF3EB;
}
.item.style2 {
    background: #F6B79C;
}
